  "homepage": "https://github.com/ajhsu/chromecast-device-emulator#readme",
  "dependencies": {
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "ajv": "^4.11.6",
    "babel": "^5.8.38",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
